                        <option id="momsignpos5">Base</option>
                        <option id="momsignpos6">Base of bowl</option>
                        <option id="momsignpos7">Base of jar</option>
                        <option id="momsignpos8">Belly</option>
                        <option id="momsignpos9">Bottom</option>
                        <option id="momsignpos10">Bottom of arrowhead</option>
                        <option id="momsignpos11">Bottom of bowl</option>
                        <option id="momsignpos12">Bottom of feet</option>
                        <option id="momsignpos13">Bottom of jar</option>
                        <option id="momsignpos14">Bottom of left leg</option>
                        <option id="momsignpos15">Bottom of tail</option>
                        <option id="momsignpos16">Cones</option>
                        <option id="momsignpos17">Cylinders at top</option>
                        <option id="momsignpos18">Front center</option>
                        <option id="momsignpos19">Front of base</option>
                        <option id="momsignpos20">Front, lower left</option>
                        <option id="momsignpos21">Inside</option>
                        <option id="momsignpos22">Inside base</option>
                        <option id="momsignpos23">Inside mask</option>
                        <option id="momsignpos24">Inside top</option>
                        <option id="momsignpos25">Interior</option>
                        <option id="momsignpos26">Lower left</option>
                        <option id="momsignpos27">Lower right</option>
                        <option id="momsignpos28">Lower right corner</option>
                        <option id="momsignpos29">Lower wall</option>
                        <option id="momsignpos30">Near bottom</option>
                        <option id="momsignpos31">On back</option>
                        <option id="momsignpos32">On back of retablo</option>
                        <option id="momsignpos33">On back, stamped</option>
                        <option id="momsignpos34">On base</option>
                        <option id="momsignpos35">On base bottom</option>
                        <option id="momsignpos36">On belly</option>
                        <option id="momsignpos37">On bottom</option>
                        <option id="momsignpos38">On bottom of base</option>
