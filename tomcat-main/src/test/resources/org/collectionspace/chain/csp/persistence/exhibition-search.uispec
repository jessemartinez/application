{
    ".csc-exhibition-exhibitionNumber": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "exhibitionNumbers",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-exhibition-exhibitionNumber": "${{row}.exhibitionNumber}"
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-exhibition-exhibitionNumber-label": {
        "messagekey": "exhibition-exhibitionNumberLabel"
    },
    ".csc-exhibition-organizer": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "organizers",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-exhibition-organizer": {
                                    "decorators": [
                                        {
                                            "func": "cspace.autocomplete",
                                            "options": {
                                                "queryUrl": "../../../tenant/core/exhibition/autocomplete/organizer",
                                                "vocabUrl": "../../../tenant/core/exhibition/source-vocab/organizer"
                                            },
                                            "type": "fluid"
                                        }
                                    ],
                                    "value": "${{row}.organizer}"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-exhibition-organizer-label": {
        "messagekey": "exhibition-organizerLabel"
    },
    ".csc-exhibition-sponsor": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
