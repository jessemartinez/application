                                    "decorators": [
                                        {
                                            "func": "cspace.autocomplete",
                                            "options": {
                                                "queryUrl": "../../../tenant/core/exhibition/autocomplete/sponsor",
                                                "vocabUrl": "../../../tenant/core/exhibition/source-vocab/sponsor"
                                            },
                                            "type": "fluid"
                                        }
                                    ],
                                    "value": "${{row}.sponsor}"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-exhibition-sponsor-label": {
        "messagekey": "exhibition-sponsorLabel"
    },
    ".csc-exhibition-title": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "titles",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-exhibition-title": "${{row}.title}"
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-exhibition-title-label": {
        "messagekey": "exhibition-titleLabel"
    },
    ".csc-exhibition-type": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "types",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-exhibition-type": {
                                    "decorators": [
                                        {
                                            "func": "cspace.termList",
                                            "options": {
                                                "elPath": "type",
                                                "root": "{row}",
                                                "termListType": "type"
                                            },
                                            "type": "fluid"
                                        }
                                    ]
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-exhibition-type-label": {
