                </field>
            </repeat>
        </section>
        <section id="nonTextualInscriptionInformation">
            <repeat id="nonTextualInscriptionGroupList/nonTextualInscriptionGroup">
                <field id="inscriptionDescriptionPosition" seperate_ui_container="true">
                    <selector>object-description-inscription-description-position</selector>
                    <!-- updating term list -->
                    <options>
                        <option id="">Please select a value</option>
                        <option id="mominscrpos1">Bottom of bowl</option>
                        <option id="mominscrpos2">Hemline</option>
                        <option id="mominscrpos3">On back of pendant</option>
                        <option id="mominscrpos4">On base</option>
                        <option id="mominscrpos5">On front of lintel</option>
                        <option id="mominscrpos6">proper left foot</option>
                    </options>
                </field>
                <field id="inscriptionDescriptionType" seperate_ui_container="true">
                    <selector>object-description-inscription-description-type</selector>
                    <!-- updating term list -->
                    <options>
                        <option id="">Please select a value</option>
                        <option id="mominscrtype1">Hallmark</option>
                        <option id="mominscrtype2">Inscription</option>
                        <option id="mominscrtype3">Paper Tag</option>
                        <option id="mominscrtype4">Signature</option>
                        <option id="mominscrtype5">Stamp</option>
                    </options>
                </field>
            </repeat>
        </section>
    </section>
    <section id="objectCollectionInformation">
        <repeat id="provenances" section="man">
            <field id="provenance" section="man">
                <selector>collection-object-provenance</selector>
            </field>
        </repeat>
    </section>
</record>