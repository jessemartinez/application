{
    ".csc-exhibition-venueClosingDateEnd": {
        "decorators": [{
            "func": "cspace.datePicker",
            "type": "fluid"
        }],
        "value": "${venueClosingDateEnd}"
    },
    ".csc-exhibition-createdAtEnd": {
        "decorators": [{
            "func": "cspace.datePicker",
            "type": "fluid"
        }],
        "value": "${createdAtEnd}"
    },
    ".csc-exhibition-venueOpeningDateEnd-label": {"messagekey": "exhibition-venueOpeningDateEndLabel"},
    ".csc-exhibition-createdAtEnd-label": {"messagekey": "exhibition-createdAtEndLabel"},
    ".csc-exhibition-venueClosingDateEnd-label": {"messagekey": "exhibition-venueClosingDateEndLabel"},
    ".csc-exhibition-venueClosingDate-label": {"messagekey": "exhibition-venueClosingDateLabel"},
    ".csc-exhibition-venueAttendance-label": {"messagekey": "exhibition-venueAttendanceLabel"},
    ".csc-exhibition-updatedAtStart": {
        "decorators": [{
            "func": "cspace.datePicker",
            "type": "fluid"
        }],
        "value": "${updatedAtStart}"
    },
    ".csc-exhibition-venueGroup": {"decorators": [{
        "func": "cspace.makeRepeatable",
        "options": {
            "repeatTree": {"expander": {
                "tree": {
                    ".csc-exhibition-venueOpeningDate": {
                        "decorators": [{
                            "func": "cspace.datePicker",
                            "type": "fluid"
                        }],
                        "value": "${{row}.venueOpeningDate}"
                    },
                    ".csc-exhibition-venue": {
                        "decorators": [{
                            "func": "cspace.autocomplete",
                            "options": {
                                "queryUrl": "../../../tenant/core/exhibition/autocomplete/venue",
                                "vocabUrl": "../../../tenant/core/exhibition/source-vocab/venue"
                            },
                            "type": "fluid"
                        }],
                        "value": "${{row}.venue}"
                    },
                    ".csc-exhibition-venueClosingDate": {
                        "decorators": [{
                            "func": "cspace.datePicker",
                            "type": "fluid"
                        }],
                        "value": "${{row}.venueClosingDate}"
                    }
                },
                "type": "fluid.noexpand"
            }},
            "elPath": "venueGroup",
            "hidePrimary": true
        },
        "type": "fluid"
    }]},
    ".csc-exhibition-sponsor-label": {"messagekey": "exhibition-sponsorLabel"},
    ".csc-exhibition-exhibitionNumber": {"decorators": [{
        "func": "cspace.makeRepeatable",
        "options": {
            "repeatTree": {"expander": {
                "tree": {".csc-exhibition-exhibitionNumber": "${{row}.exhibitionNumber}"},
                "type": "fluid.noexpand"
            }},
            "elPath": "exhibitionNumbers",
            "hidePrimary": true
        },
        "type": "fluid"
    }]},
    ".csc-exhibition-type": {"decorators": [{
        "func": "cspace.makeRepeatable",
        "options": {
            "repeatTree": {"expander": {
                "tree": {".csc-exhibition-type": {"decorators": [{
                    "func": "cspace.termList",
                    "options": {
                        "termListType": "type",
                        "root": "{row}",
                        "elPath": "type"
                    },
                    "type": "fluid"
                }]}},
                "type": "fluid.noexpand"
            }},
            "elPath": "types",
            "hidePrimary": true
        },
        "type": "fluid"
    }]},
    ".csc-exhibition-updatedBy-label": {"messagekey": "exhibition-updatedByLabel"},
    ".csc-exhibition-venueClosingDateStart": {
        "decorators": [{
            "func": "cspace.datePicker",
            "type": "fluid"
        }],
        "value": "${venueClosingDateStart}"
    },
    ".csc-exhibition-organizer": {"decorators": [{
        "func": "cspace.makeRepeatable",
        "options": {
            "repeatTree": {"expander": {
                "tree": {".csc-exhibition-organizer": {
                    "decorators": [{
                        "func": "cspace.autocomplete",
                        "options": {
                            "queryUrl": "../../../tenant/core/exhibition/autocomplete/organizer",
                            "vocabUrl": "../../../tenant/core/exhibition/source-vocab/organizer"
                        },
                        "type": "fluid"
                    }],
                    "value": "${{row}.organizer}"
                }},
                "type": "fluid.noexpand"
            }},
            "elPath": "organizers",
            "hidePrimary": true
        },
        "type": "fluid"
    }]},
    ".csc-exhibition-venueOpeningDateStart": {
        "decorators": [{
            "func": "cspace.datePicker",
            "type": "fluid"
        }],
        "value": "${venueOpeningDateStart}"
    },
    ".csc-exhibition-title-label": {"messagekey": "exhibition-titleLabel"},
    ".csc-exhibition-type-label": {"messagekey": "exhibition-typeLabel"},
    ".csc-exhibition-organizer-label": {"messagekey": "exhibition-organizerLabel"},
    ".csc-exhibition-venueUrl-label": {"messagekey": "exhibition-venueUrlLabel"},
    ".csc-exhibition-updatedAtEnd-label": {"messagekey": "exhibition-updatedAtEndLabel"},
    ".csc-exhibition-createdAtStart": {
        "decorators": [{
            "func": "cspace.datePicker",
            "type": "fluid"
        }],
        "value": "${createdAtStart}"
    },
    ".csc-exhibition-sponsor": {"decorators": [{
        "func": "cspace.makeRepeatable",
        "options": {
            "repeatTree": {"expander": {
                "tree": {".csc-exhibition-sponsor": {
                    "decorators": [{
                        "func": "cspace.autocomplete",
                        "options": {
                            "queryUrl": "../../../tenant/core/exhibition/autocomplete/sponsor",
                            "vocabUrl": "../../../tenant/core/exhibition/source-vocab/sponsor"
                        },
                        "type": "fluid"
                    }],
                    "value": "${{row}.sponsor}"
                }},
                "type": "fluid.noexpand"
            }},
            "elPath": "sponsors",
            "hidePrimary": true
        },
        "type": "fluid"
    }]},
    ".csc-exhibition-venueOpeningDateEnd": {
        "decorators": [{
            "func": "cspace.datePicker",
            "type": "fluid"
        }],
        "value": "${venueOpeningDateEnd}"
    },
    ".csc-exhibition-updatedBy": {"decorators": [{
        "func": "cspace.makeRepeatable",
        "options": {
            "repeatTree": {"expander": {
                "tree": {".csc-exhibition-updatedBy": "${{row}.updatedBy}"},
                "type": "fluid.noexpand"
            }},
            "elPath": "updatedBys",
            "hidePrimary": true
        },
        "type": "fluid"
    }]},
    ".csc-exhibition-title": {"decorators": [{
        "func": "cspace.makeRepeatable",
        "options": {
            "repeatTree": {"expander": {
                "tree": {".csc-exhibition-title": "${{row}.title}"},
                "type": "fluid.noexpand"
            }},
            "elPath": "titles",
            "hidePrimary": true
        },
        "type": "fluid"
    }]},
    ".csc-exhibition-venueOpeningDate-label": {"messagekey": "exhibition-venueOpeningDateLabel"},
    ".csc-exhibition-venue-label": {"messagekey": "exhibition-venueLabel"},
    ".csc-exhibition-updatedAtEnd": {
        "decorators": [{
            "func": "cspace.datePicker",
            "type": "fluid"
        }],
        "value": "${updatedAtEnd}"
    },
    ".csc-exhibition-createdAtStart-label": {"messagekey": "exhibition-createdAtStartLabel"},
    ".csc-exhibition-exhibitionNumber-label": {"messagekey": "exhibition-exhibitionNumberLabel"},
    ".csc-exhibition-venueOpeningDateStart-label": {"messagekey": "exhibition-venueOpeningDateStartLabel"},
    ".csc-exhibition-venueClosingDateStart-label": {"messagekey": "exhibition-venueClosingDateStartLabel"},
    ".csc-exhibition-createdAt-label": {"messagekey": "exhibition-createdAtLabel"},
    ".csc-exhibition-updatedAtStart-label": {"messagekey": "exhibition-updatedAtStartLabel"},
    ".csc-exhibition-updatedAt-label": {"messagekey": "exhibition-updatedAtLabel"}
}