        "messagekey": "exhibition-updatedAtLabel"
    },
    ".csc-exhibition-updatedAtEnd": {
        "decorators": [
            {
                "func": "cspace.datePicker",
                "type": "fluid"
            }
        ],
        "value": "${updatedAtEnd}"
    },
